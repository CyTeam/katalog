%h1
  = t('katalog.main_navigation.changes')

= paginated_section :renderer => PaginationListLinkRenderer do
  .ce_table.block
    %table
      %tbody
        %tr
          %th
            = t_attr(:item_type)
          %th
            = t_attr(:object)
          %th
            = t_attr(:changed_from)
          %th
            = t_attr(:changed_at)
          %th
            = t_attr(:action)
          %th

        = render @versions
