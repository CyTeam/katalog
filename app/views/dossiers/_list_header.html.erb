<tr class="header">
  <th class="signature"><%= t_attr('signature', Dossier) %></th>
  <th class="title" colspan="2">
    <%= t_attr('title') %>
    <span class="document_count" style="float: right">
      <%= @document_count ? t('katalog.total_count', :count => number_with_delimiter(@document_count)) : t_attr(:document_count) %>
    </span>
  </th>
  <th class="action-links"></th>
</tr>
