= render 'search_form'

#edit_report_header
  %h1= t('katalog.main_navigation.edit_year')

  %label= t('katalog.years_amount')
  = select 'dossier_numbers', 'year_amount', 1..10, {:selected => @years.count}

= paginated_section :renderer => PaginationListLinkRenderer do
  #edit_report.ce_table.block
    %table
      = render 'batch_edit_last_year/list_header'
      - @tabindex = 1
      = render :partial => 'batch_edit_last_year/dossier', :collection => @dossiers
