<% cache dossier.cache_key + @new_signature.to_s do %>
<tr id="dossier-<%= dossier.id %>" class="<%= cycle('odd', 'even') %>" data-href="<%= url_for dossier %>">
  <td><%= @new_signature ? dossier.new_signature : dossier.signature %></td>
  <td><%= dossier.title %></td>
  <td><%= dossier.kind %></td>
  <td><%= dossier.location %></td>
  <td><%= dossier.total_amount %></td>
  <td style="width: 70px">
    <%= link_to image_tag('icons/edit.png', :title => 'Edit'), edit_dossier_path(dossier) %>
    <%= link_to image_tag('icons/delete.png', :title => 'Destroy'), dossier, :confirm => 'Are you sure?', :remote => true, :method => :delete %>
  </td>
</tr>
<% end %>
