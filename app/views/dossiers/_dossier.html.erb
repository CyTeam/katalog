<tr id="dossier-<%= dossier.id %>" class="dossier <%= cycle('odd', 'even') %>" data-href="<%= url_for dossier %>">
  <td><%= @new_signature ? dossier.new_signature : dossier.signature %></td>
  <td><%= dossier.title %></td>
  <td><%= dossier.kind %></td>
  <td><%= dossier.locations.map{|location| location.code}.join(', ') %></td>
  <td style="text-align: right"><%= dossier.document_count %></td>
  <td style="width: 70px">
    <% if user_signed_in? %>
      <%= link_to image_tag('icons/edit.png', :title => 'Edit'), edit_dossier_path(dossier) %>
      <%= link_to image_tag('icons/delete.png', :title => 'Destroy'), dossier, :confirm => 'Are you sure?', :remote => true, :method => :delete %>
    <% end %>
  </td>
</tr>
