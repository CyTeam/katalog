<%= render 'search_form' %>

<%= render 'layouts/page_actions' %>
<div id="page_actions" class="contextual">
  <%= contextual_link 'report', report_dossiers_path(:format => :pdf, :per_page => 'all', :search => params[:search]), :title => t('tooltips.dossiers.show_report') %>
  <%= contextual_link 'year_report', year_report_path(:format => :pdf, :per_page => 'all', :search => params[:search]), :title => t('tooltips.dossiers.show_year_report') %>
</div>
<%- if @dossiers.present? %>
  <%= render 'search_title' %>
  <%= paginated_section :renderer => PaginationListLinkRenderer, :show_always => @paginated_scope.present? do %>
    <%= render 'list' %>
  <%- end %>
<%- elsif @query %>
  <h1><%= t('katalog.nothing_found.title') %></h1>
  <p><%= t('katalog.nothing_found.text') %></p>
<%- else %>
  <h1><%= t('katalog.search') %></h1>
<%- end %>

<%= highlight_words(@query) %>
