= render 'search_title'
.index
  #dossiers.ce_table.block.overflow-scroll.landscape-table
    %table
      %tr.header
        %th.signature
          = t_attr('signature', Dossier)
        %th.title{:style => 'width: 450px;'}
          = t_attr('title', Dossier)
        - [:first_document_year, :container_type, :location, :theme, :keyword_text].each do |attr|
          %th{:class => attr.to_s}
            = t_attr(attr.to_s, Dossier)
        
      = render :partial => @dossiers, :locals => {:report => true}


