%tr[dossier]{:class => cycle('odd', 'even')}
  %td= dossier.signature
  %td
    %span.title
      = dossier.title
  %td
    %input.dossier-last-container-title{:tabindex => @tabindex, 'data-dossier' => dossier.id, 'data-number' => dossier.numbers.last.id, :type => 'text', :value => dossier.numbers.last.amount}
    - @tabindex += 1
