%tr[dossier]{:class => cycle('odd', 'even')}
  %td.edit_dossiers_td= dossier.signature
  %td.edit_dossiers_td
    %span.title= dossier.title
  - @years.each do |year|
    %td.edit_dossiers_td
      - if number = dossier.dossier_number_from_year(year)
        %input.dossier-last-container-title{:tabindex => @tabindex,
                                            :type => 'text',
                                            :value => number.amount,
                                            :size => 3,
                                            :data => {:dossier => dossier.id,
                                                      :number => number.id}}
      - else
        %input.dossier-last-container-title{:tabindex => @tabindex,
                                            :type => 'text',
                                            :size => 3,
                                            :data => {:dossier => dossier.id,
                                                      :number => {:year => year}}}
      - @tabindex += 1
